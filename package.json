{
  "name": "vue-cli-plugin-jest-puppeteer",
  "version": "0.1.1",
  "description": "Vue CLI 3 plugin for writing E2E tests with jest and puppeteer",
  "main": "index.js",
  "scripts": {
    "eslint": "node ./node_modules/.bin/eslint --ext .js .",
    "test": "npm run eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaizendorks/vue-cli-plugin-jest-puppeteer.git"
  },
  "keywords": [
    "vue",
    "vue-cli",
    "vue-cli-plugin",
    "jest",
    "puppeteer",
    "jest-puppeteer"
  ],
  "author": "Kaizen Dorks <kaizendorks@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaizendorks/vue-cli-plugin-jest-puppeteer/issues"
  },
  "homepage": "https://github.com/kaizendorks/vue-cli-plugin-jest-puppeteer#readme",
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "husky": "1.3.1"
  }
}
